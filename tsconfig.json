{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "mini-jsx",
    "lib": ["dom", "es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "esnext",
    "types": ["jest"],
    "paths": {
      "mini-jsx": ["./jsx-runtime"],
      "mini-jsx/jsx-runtime": ["./jsx-runtime"],
    }
  }
}
